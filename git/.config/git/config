[init]
    defaultBranch = master

[user]
    name = skeletony007
    email = skeletony007@users.noreply.github.com
    signingkey = key::ssh-rsa  skeletony007

[gpg]
    format = ssh

[commit]
    gpgsign = true
    verbose = true

[alias]
    st = status -s
    lol = log --oneline --graph --all

[diff]
    algorithm = histogram
    renames = copies

[includeif "gitdir:~/personal/"]
    path = ~/.config/git/config-personal
[includeif "gitdir:~/work/"]
    path = ~/.config/git/config-work
